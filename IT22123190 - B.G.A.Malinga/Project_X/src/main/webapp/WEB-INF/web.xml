<?xml version="1.0" encoding="UTF-8"?>
<element>
<web-app 	xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
			version="3.1">
			
		<servlet>
			<servlet-name>contact</servlet-name>
			<servlet-class>pack_contact.contact_servlet</servlet-class>
		</servlet>	
		<servlet-mapping>
			<servlet-name>contact</servlet-name>
			<url-pattern>contact</url-pattern>
		</servlet-mapping>	
		
		
		
		<servlet>
			<servlet-name>view_servlet</servlet-name>
			<servlet-class>pack_contact.view_servlet</servlet-class>
		</servlet>	
		
		<servlet-mapping>
			<servlet-name>view_servlet</servlet-name>
			<url-pattern>/read</url-pattern>
		</servlet-mapping>
		
		
		
		
		<servlet>
			<servlet-name>update</servlet-name>
			<servlet-class>pack_contact.update_servlet</servlet-class>
		</servlet>	
		<servlet-mapping>
			<servlet-name>update</servlet-name>
			<url-pattern>/update</url-pattern>
		</servlet-mapping>	
		
		
		
		
		<servlet>
        	<servlet-name>delete_servlet</servlet-name>
        	<servlet-class>pack_contact.delete_servlet</servlet-class>
        </servlet>
        
        <servlet-mapping>
        	<servlet-name>delete_servlet</servlet-name>
        	<url-pattern>delete</url-pattern>
        </servlet-mapping>
			

</web-app></element>



			