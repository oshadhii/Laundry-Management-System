<?xml version="1.0" encoding="UTF-8"?>
<element>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         
         <!-- Login linking -->
         <servlet>
			<servlet-name>login</servlet-name>
			<servlet-class>login.LoginServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>login</servlet-name>
			<url-pattern>/login</url-pattern>
		</servlet-mapping>
		
		
		<!-- Order page -->
		<servlet>
			<servlet-name>insertOrder</servlet-name>
			<servlet-class>web.OrderServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>insertOrder</servlet-name>
			<url-pattern>/insert</url-pattern>
		</servlet-mapping>
		
		
		<!-- Contact page -->
		<servlet>
			<servlet-name>insertContact</servlet-name>
			<servlet-class>pack_contact.contact_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>insertContact</servlet-name>
			<url-pattern>/Contactinsert</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>viewContact</servlet-name>
			<servlet-class>pack_contact.view_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>viewContact</servlet-name>
			<url-pattern>/Contactview</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>deleteContact</servlet-name>
			<servlet-class>pack_contact.delete_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>deleteContact</servlet-name>
			<url-pattern>/Contactdelete</url-pattern>
		</servlet-mapping>
		
		<!-- Employee page - Admin side -->
		<servlet>
			<servlet-name>insertEmployee</servlet-name>
			<servlet-class>emolyee.empForm_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>insertEmployee</servlet-name>
			<url-pattern>/Employeeinsert</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>viewEmployee</servlet-name>
			<servlet-class>emolyee.view_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>viewEmployee</servlet-name>
			<url-pattern>/Employeeview</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>updateEmployee</servlet-name>
			<servlet-class>emolyee.update_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>updateEmployee</servlet-name>
			<url-pattern>/Employeeupdate</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>deleteEmployee</servlet-name>
			<servlet-class>emolyee.delete_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>deleteEmployee</servlet-name>
			<url-pattern>/Employeedelete</url-pattern>
		</servlet-mapping>
		
		<!-- Customer page - Admin side -->
		<servlet>
			<servlet-name>insertCustomer</servlet-name>
			<servlet-class>pack_customer.customer_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>insertCustomer</servlet-name>
			<url-pattern>/Customerview</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>viewCustomer</servlet-name>
			<servlet-class>pack_customer.view_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>viewCustomer</servlet-name>
			<url-pattern>/Customerview</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>updateCustomer</servlet-name>
			<servlet-class>pack_customer.update_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>updateCustomer</servlet-name>
			<url-pattern>/Customerupdate</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>deleteCustomer</servlet-name>
			<servlet-class>pack_customer.delete_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>deleteCustomer</servlet-name>
			<url-pattern>/Customerdelete</url-pattern>
		</servlet-mapping>
		
</web-app>
</element>